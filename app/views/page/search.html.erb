<h2>Search Results</h2>
<br>
<h3>Listings</h3>
<% if @listings.nil? %>
No results found.
<% else %>
<% @listings.each do |listing| %>
<p>
  <%= link_to User.find(listing.user_id).name, User.find(listing.user_id) %>
  <%= link_to listing.address, listing %>
  <%= listing.city %>
  <%= listing.state %>
  <%= listing.zipcode %>
</p>
<% end %>
<% end %>
